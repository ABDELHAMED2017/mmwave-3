+
+EpcS1APErabReleaseIndicationHeader::EpcS1APErabReleaseIndicationHeader ()
+  : m_numberOfIes (1 + 1 + 1),
+    m_headerLength (9 + 3 + 4 + 1),
+    m_enbUeS1Id (0xfffa),
+    m_mmeUeS1Id (0xfffffffa)
