+{ 
+  m_numberOfIes = 0;
+  m_headerLength = 0;
+  m_stmsi = 0xfffffffb;
+  m_enbUeS1Id = 0xfffb;
+  m_ecgi = 0xfffb;
+  m_mmeUeS1Id = 0xfffffffb;
+}
