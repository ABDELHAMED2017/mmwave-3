+TypeId
+EpcS1APInitialUeMessageHeader::GetTypeId (void)
+{
+  static TypeId tid = TypeId ("ns3::EpcS1APInitialUeMessageHeader")
+    .SetParent<Header> ()
+    .SetGroupName("Lte")
+    .AddConstructor<EpcS1APInitialUeMessageHeader> ()
+  ;
+  return tid;
+}
