-  // Convert bandwidth from RBs to kHz
-  uint32_t maxBandwidthKhz = LteSpectrumValueHelper::GetChannelBandwidth(maxBandwidthRb) / 1e3;
-
-  for (uint8_t i = 0; i < m_numberOfComponentCarriers; i++)
