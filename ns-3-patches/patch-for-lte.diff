+  os << "MmeUeS1apId = " << m_mmeUeS1Id;
+  os << " EnbUeS1Id = " << m_enbUeS1Id;
+  os << " ECGI = " << m_ecgi;
+  os << " S-TMSI = " << m_stmsi;
